<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8">
  <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">
  <meta name="description" content="<%= page.description || theme.description || config.description %>">
  <meta name="keyword" content="<%= config.keyword || theme.keyword %>">
  <% if (config.favicon || theme.favicon){ %>
    <link rel="shortcut icon" type="image/ico" href="<%= config.favicon || theme.favicon %>"/>
  <% } else { %>
    <%- favicon_tag('css/images/logo.png') %>
  <% } %>
  <title><% if (page.title){ %><%= page.title %> | <%= config.title %><% } else { %><%= config.title %><% } %></title>
  <%- open_graph({
    twitter_id: theme.about.twitter_username
  }) %>
  <%- css(['css/style', 'css/plugins/font-awesome-4.7.0.min', 'css/plugins/highlight.js-9.12.0.min', 'css/plugins/nprogress-0.2.0.min']) %>
  <%- js(['js/jquery-3.4.1.min', 'js/geopattern-1.2.3.min', 'js/nprogress-0.2.0.min']) %>
  <%- partial('_third-party/google-analytics') %>
  <%- partial('_third-party/mathjax') %>
</head>
